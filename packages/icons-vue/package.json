{
  "name": "@nutui/icons-vue",
  "version": "0.0.15",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build --config vite.config.build.umd.ts && vite build --config vite.config.build.es.ts && vite build --config vite.config.build.css.ts",
    "build:site": "vue-tsc && vite build",
    "preview": "vite preview",
    "release": "release-it"
  },
  "main": "dist/lib/index.umd.cjs",
  "module": "dist/es/index.es.js",
  "style": "dist/style_icon.css",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "package.json",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "vue": "^3.2.41"
  },
  "author": "jdf2e",
  "license": "MIT",
  "release-it": {
    "git": {
      "tag": false,
      "commitMessage": "release: @nutui/icons-vue ${version}",
      "addUntrackedFiles": true,
      "requireCleanWorkingDir": false
    }
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.2.0",
    "release-it": "^15.5.1",
    "sass": "^1.56.1",
    "typescript": "^4.6.4",
    "vite": "^3.2.3",
    "vue-tsc": "^1.0.10"
  }
}
