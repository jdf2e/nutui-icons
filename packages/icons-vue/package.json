{
  "name": "@nutui/icons-vue",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "release": "release-it"
  },
  "main": "./dist/icon.umd.cjs",
  "module": "./dist/icon.js",
  "exports": {
    ".": {
      "import": "./dist/icon.js",
      "require": "./dist/icon.umd.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "package.json",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "vue": "^3.2.41"
  },
  "author": "richard1015",
  "license": "MIT",
  "release-it": {
    "git": {
      "tag": false,
      "commitMessage": "release: @nutui/icons-vue ${version}",
      "addUntrackedFiles": true,
      "requireCleanWorkingDir": false
    }
  },
  "devDependencies": {
    "@types/node": "^18.11.10",
    "@vitejs/plugin-vue": "^3.2.0",
    "release-it": "^15.5.1",
    "sass": "^1.56.1",
    "typescript": "^4.6.4",
    "vite": "^3.2.3",
    "vue-tsc": "^1.0.9"
  }
}
