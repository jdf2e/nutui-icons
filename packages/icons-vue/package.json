{
  "name": "@nutui/icons-vue",
  "version": "0.0.26",
  "description": "京东风格的轻量级移动端 Vue 组件库 NutUI 图标包",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build --config vite.config.build.umd.ts && vite build --config vite.config.build.es.ts && vite build --config vite.config.build.css.ts",
    "build:site": "vue-tsc && vite build",
    "preview": "vite preview",
    "release": "release-it",
    "release:beta": "release-it --preRelease=beta"
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/es/index.es.js",
      "require": "./dist/lib/index.umd.js"
    },
    "./*": "./*"
  },
  "main": "dist/lib/index.umd.js",
  "module": "dist/es/index.es.js",
  "style": "dist/style_icon.css",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "package.json",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "author": "jdf2e",
  "license": "MIT",
  "release-it": {
    "git": {
      "tag": false,
      "commitMessage": "release: @nutui/icons-vue ${version}",
      "addUntrackedFiles": true,
      "requireCleanWorkingDir": false
    }
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "release-it": "^15.5.1",
    "sass": "^1.56.1",
    "typescript": "^4.6.4",
    "vite": "^4.1.4",
    "vue": "^3.2.41",
    "vue-tsc": "^1.0.10"
  }
}
